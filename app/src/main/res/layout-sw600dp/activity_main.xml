<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/background_light"
    android:baselineAligned="false"
    android:focusable="true"
    android:focusableInTouchMode="true"
    android:orientation="horizontal"
    android:weightSum="100"
    tools:context="ru.ralnik.ingrad.MainActivity">


    <LinearLayout
        android:id="@+id/leftPanel"
        android:layout_width="178dp"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1">

            <ImageView
                android:id="@+id/imgLogo"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:adjustViewBounds="true"
                android:contentDescription="TODO"
                android:scaleType="fitXY"
                android:visibility="visible"
                app:srcCompat="@drawable/bg" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:scrollY="-130dp">

                <ImageView
                    android:id="@+id/btnFull"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:adjustViewBounds="true"
                    android:onClick="leftPanelButtonOnCLick"
                    app:srcCompat="@drawable/button_full" />

                <ImageView
                    android:id="@+id/btnLocation"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:adjustViewBounds="true"
                    android:onClick="leftPanelButtonOnCLick"
                    app:srcCompat="@drawable/button_location" />

                <ImageView
                    android:id="@+id/btnAbout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:adjustViewBounds="true"
                    android:onClick="leftPanelButtonOnCLick"
                    app:srcCompat="@drawable/button_about" />

                <ImageView
                    android:id="@+id/btnTransport"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:adjustViewBounds="true"
                    android:onClick="leftPanelButtonOnCLick"
                    app:srcCompat="@drawable/button_transport" />

                <ImageView
                    android:id="@+id/btnArea"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:adjustViewBounds="true"
                    android:onClick="leftPanelButtonOnCLick"
                    app:srcCompat="@drawable/button_area" />

                <ImageView
                    android:id="@+id/btnNaturals"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:adjustViewBounds="true"
                    android:onClick="leftPanelButtonOnCLick"
                    app:srcCompat="@drawable/button_naturales" />

                <ImageView
                    android:id="@+id/btnInfra"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:adjustViewBounds="true"
                    android:onClick="leftPanelButtonOnCLick"
                    app:srcCompat="@drawable/button_infra" />

                <ImageView
                    android:id="@+id/btnOutsideInfra"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:adjustViewBounds="true"
                    android:onClick="leftPanelButtonOnCLick"
                    app:srcCompat="@drawable/button_outside_infra" />

                <ImageView
                    android:id="@+id/btnTorpedo"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:adjustViewBounds="true"
                    android:onClick="leftPanelButtonOnCLick"
                    app:srcCompat="@drawable/button_torpedo" />

                <ImageView
                    android:id="@+id/btnSurround"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:adjustViewBounds="true"
                    android:onClick="leftPanelButtonOnCLick"
                    app:srcCompat="@drawable/button_surround" />

                <ImageView
                    android:id="@+id/btnAdvantageProject"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:adjustViewBounds="true"
                    android:onClick="leftPanelButtonOnCLick"
                    app:srcCompat="@drawable/button_advantage_project" />

                <ImageView
                    android:id="@+id/btnGallary"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:adjustViewBounds="true"
                    android:onClick="leftPanelButtonOnCLick"
                    app:srcCompat="@drawable/button_gallary" />

                <ImageView
                    android:id="@+id/btnProcessBuildings"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:adjustViewBounds="true"
                    android:onClick="leftPanelButtonOnCLick"
                    app:srcCompat="@drawable/button_process_buildings" />
            </LinearLayout>

        </FrameLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/rightPanel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginEnd="32dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="32dp"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/audioPanel"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/rightpanel_control_margin_bottom"
            android:gravity="end"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/btnPlayPause"
                android:layout_width="58dp"
                android:layout_height="68dp"
                android:layout_marginEnd="32dp"
                android:adjustViewBounds="true"
                android:onClick="conlrolPanelButtonOnClick"
                app:srcCompat="@drawable/button_playpause" />

            <ImageView
                android:id="@+id/btnVolume"
                android:layout_width="58dp"
                android:layout_height="68dp"
                android:layout_marginEnd="32dp"
                android:adjustViewBounds="true"
                android:onClick="conlrolPanelButtonOnClick"
                app:srcCompat="@drawable/button_volume" />

            <ImageView
                android:id="@+id/btnOptions"
                android:layout_width="58dp"
                android:layout_height="68dp"
                android:adjustViewBounds="true"
                app:srcCompat="@drawable/button_options_pressed"
                android:onClick="conlrolPanelButtonOnClick"/>

        </LinearLayout>

        <ScrollView
            android:id="@+id/scrollViewRight"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:visibility="visible">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/filterLayout"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/imgGenplan"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/genplan_margin_bottom"
                        android:layout_weight="1"
                        android:adjustViewBounds="true"
                        app:srcCompat="@drawable/genplan" />

                    <ImageView
                        android:id="@+id/imgTitleCountRoom"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/titles_margin_top"
                        android:adjustViewBounds="true"
                        app:srcCompat="@drawable/title_count_room" />

                    <LinearLayout
                        android:id="@+id/barCountRoom"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="@dimen/bar_countroom_margin_bottom"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/btnRoom1"
                            android:layout_width="61dp"
                            android:layout_height="61dp"
                            android:layout_marginEnd="@dimen/button_room_margin_right"
                            android:adjustViewBounds="true"
                            android:onClick="countRoomButtonOnClick"
                            app:srcCompat="@drawable/button_room1" />

                        <ImageView
                            android:id="@+id/btnRoom2"
                            android:layout_width="61dp"
                            android:layout_height="61dp"
                            android:layout_marginEnd="@dimen/button_room_margin_right"
                            android:adjustViewBounds="true"
                            android:onClick="countRoomButtonOnClick"
                            app:srcCompat="@drawable/button_room2" />

                        <ImageView
                            android:id="@+id/btnRoom3"
                            android:layout_width="61dp"
                            android:layout_height="61dp"
                            android:layout_marginEnd="@dimen/button_room_margin_right"
                            android:adjustViewBounds="true"
                            android:onClick="countRoomButtonOnClick"
                            app:srcCompat="@drawable/button_room3" />

                        <ImageView
                            android:id="@+id/btnRoom4"
                            android:layout_width="61dp"
                            android:layout_height="61dp"
                            android:layout_marginEnd="@dimen/button_room_margin_right"
                            android:adjustViewBounds="true"
                            android:onClick="countRoomButtonOnClick"
                            app:srcCompat="@drawable/button_room4" />

                        <ImageView
                            android:id="@+id/btnRoom5"
                            android:layout_width="61dp"
                            android:layout_height="61dp"
                            android:layout_marginEnd="@dimen/button_room_margin_right"
                            android:adjustViewBounds="true"
                            android:onClick="countRoomButtonOnClick"
                            app:srcCompat="@drawable/button_room5" />

                        <ImageView
                            android:id="@+id/btnRoomEvro"
                            android:layout_width="61dp"
                            android:layout_height="61dp"
                            android:adjustViewBounds="true"
                            android:onClick="countRoomButtonOnClick"
                            app:srcCompat="@drawable/button_room_evro" />
                    </LinearLayout>

                    <ImageView
                        android:id="@+id/titleFloor"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/titles_before_seekbar_margin_bottom"
                        android:layout_weight="1"
                        app:srcCompat="@drawable/title_floor" />

                    <com.crystal.crystalrangeseekbar.widgets.CrystalRangeSeekbar
                        android:id="@+id/seekBarFloor"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        app:bar_color="@color/seekbarColor"
                        app:bar_highlight_color="@color/seekbarColor"
                        app:corner_radius="1"
                        app:data_type="_integer"
                        app:left_thumb_color="@color/seekbarColor"
                        app:left_thumb_color_pressed="@color/seekbarColor"
                        app:max_value="55"
                        app:min_value="2"
                        app:right_thumb_color="@color/seekbarColor"
                        app:right_thumb_color_pressed="@color/seekbarColor" />

                    <LinearLayout
                        android:id="@+id/floorLayoutSelector"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TableRow
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_margin="2dip">

                            <EditText
                                android:id="@+id/minFloorEdit"
                                android:background="@color/colorEditOfSeekBar"
                                android:ems="4"
                                android:gravity="center"
                                android:inputType="number"
                                android:text="2"
                                android:textColor="@color/colorTextEditOfSeekBar"
                                android:textSize="16sp" />

                        </TableRow>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="1" />

                        <TableRow
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_margin="2dip">

                            <EditText
                                android:id="@+id/maxFloorEdit"
                                android:background="@color/colorEditOfSeekBar"
                                android:ems="4"
                                android:gravity="center"
                                android:inputType="number"
                                android:text="55"
                                android:textColor="@color/colorTextEditOfSeekBar"
                                android:textSize="16sp" />
                        </TableRow>
                    </LinearLayout>

                    <ImageView
                        android:id="@+id/titleCost"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/titles_margin_top"
                        android:layout_marginBottom="@dimen/titles_before_seekbar_margin_bottom"
                        android:layout_weight="1"
                        app:srcCompat="@drawable/title_cost" />

                    <com.crystal.crystalrangeseekbar.widgets.CrystalRangeSeekbar
                        android:id="@+id/seekBarCost"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        app:bar_color="@color/seekbarColor"
                        app:bar_highlight_color="@color/seekbarColor"
                        app:data_type="_float"
                        app:left_thumb_color="@color/seekbarColor"
                        app:max_value="115.5"
                        app:min_value="31.5"
                        app:right_thumb_color="@color/seekbarColor" />

                    <LinearLayout
                        android:id="@+id/costLayoutSelector"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TableRow
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_margin="2dip">

                            <EditText
                                android:id="@+id/minCostEdit"
                                android:background="@color/colorEditOfSeekBar"
                                android:ems="4"
                                android:gravity="center"
                                android:inputType="numberDecimal"
                                android:text="453"
                                android:textColor="@color/colorTextEditOfSeekBar"
                                android:textSize="16sp" />

                        </TableRow>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="1" />

                        <TableRow
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_margin="2dip">

                            <EditText
                                android:id="@+id/maxCostEdit"
                                android:background="@color/colorEditOfSeekBar"
                                android:ems="4"
                                android:gravity="center"
                                android:inputType="numberDecimal"
                                android:text="123"
                                android:textColor="@color/colorTextEditOfSeekBar"
                                android:textSize="16sp" />
                        </TableRow>

                    </LinearLayout>

                    <ImageView
                        android:id="@+id/titleSqure"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/titles_margin_top"
                        android:layout_marginBottom="@dimen/titles_before_seekbar_margin_bottom"
                        android:layout_weight="1"
                        app:srcCompat="@drawable/title_square" />

                    <com.crystal.crystalrangeseekbar.widgets.CrystalRangeSeekbar
                        android:id="@+id/seekBarSquare"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        app:bar_color="@color/seekbarColor"
                        app:bar_highlight_color="@color/seekbarColor"
                        app:data_type="_float"
                        app:left_thumb_color="@color/seekbarColor"
                        app:max_value="115.5"
                        app:min_value="31.5"
                        app:right_thumb_color="@color/seekbarColor" />

                    <LinearLayout
                        android:id="@+id/squareLayoutSelector"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TableRow
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_margin="2dip">

                            <EditText
                                android:id="@+id/minSquareEdit"
                                android:background="@color/colorEditOfSeekBar"
                                android:ems="4"
                                android:gravity="center"
                                android:inputType="numberDecimal"
                                android:text="453"
                                android:textColor="@color/colorTextEditOfSeekBar"
                                android:textSize="16sp" />

                        </TableRow>

                        <TextView
                            android:id="@+id/textView5"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="1" />

                        <TableRow
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_margin="2dip">

                            <EditText
                                android:id="@+id/maxSquareEdit"
                                android:background="@color/colorEditOfSeekBar"
                                android:ems="4"
                                android:gravity="center"
                                android:inputType="numberDecimal"
                                android:text="123"
                                android:textColor="@color/colorTextEditOfSeekBar"
                                android:textSize="16sp" />
                        </TableRow>

                    </LinearLayout>

                    <ImageView
                        android:id="@+id/titleSrokEnter"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/titles_margin_top"
                        android:layout_marginBottom="@dimen/titles_margin_bottom"
                        android:layout_weight="1"
                        app:srcCompat="@drawable/title_srok_enter" />

                    <LinearLayout
                        android:id="@+id/barSrokEnter"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/btnSrok_2_2022"
                            android:layout_width="89dp"
                            android:layout_height="23dp"
                            android:layout_marginEnd="125dp"
                            android:adjustViewBounds="true"
                            app:srcCompat="@drawable/checkbox_srok_2_2022"
                            android:onClick="periodOnClick"/>

                        <ImageView
                            android:id="@+id/btnSrok_3_2022"
                            android:layout_width="89dp"
                            android:layout_height="23dp"
                            android:layout_marginEnd="125dp"
                            android:adjustViewBounds="true"
                            app:srcCompat="@drawable/checkbox_srok_3_2022" />

                        <ImageView
                            android:id="@+id/btnSrok_4_2022"
                            android:layout_width="89dp"
                            android:layout_height="23dp"
                            android:layout_gravity="end"
                            android:adjustViewBounds="true"
                            app:srcCompat="@drawable/checkbox_srok_4_2022" />


                    </LinearLayout>

                    <ImageView
                        android:id="@+id/titleRemont"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/titles_margin_top"
                        android:layout_marginBottom="@dimen/titles_margin_bottom"
                        android:layout_weight="1"
                        app:srcCompat="@drawable/title_remont" />

                    <LinearLayout
                        android:id="@+id/barButtonsRemont"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/btnWithoutRemont"
                            android:layout_width="86dp"
                            android:layout_height="79dp"
                            android:layout_marginEnd="@dimen/buttons_remont_margin_right"
                            android:adjustViewBounds="true"
                            android:onClick="remontButtonOnClick"
                            app:srcCompat="@drawable/button_without_remont" />

                        <ImageView
                            android:id="@+id/btnWithRemont"
                            android:layout_width="86dp"
                            android:layout_height="79dp"
                            android:layout_marginEnd="@dimen/buttons_remont_margin_right"
                            android:adjustViewBounds="true"
                            android:onClick="remontButtonOnClick"
                            app:srcCompat="@drawable/button_with_remont" />

                        <ImageView
                            android:id="@+id/btnWhiteBox"
                            android:layout_width="86dp"
                            android:layout_height="79dp"
                            android:layout_marginRight="@dimen/buttons_remont_margin_right"
                            android:adjustViewBounds="true"
                            android:onClick="remontButtonOnClick"
                            app:srcCompat="@drawable/button_remont_whitebox" />

                    </LinearLayout>

                    <ImageView
                        android:id="@+id/titleAdditionalAttributes"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/titles_margin_top"
                        android:layout_marginBottom="@dimen/titles_margin_bottom"
                        android:layout_weight="1"
                        app:srcCompat="@drawable/title_additional_attribute" />

                    <LinearLayout
                        android:id="@+id/barAdditionalAttributes1"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="@dimen/buttons1_house_margin_bottom"
                        android:orientation="horizontal"
                        android:weightSum="100">

                        <ImageView
                            android:id="@+id/btnTownHouse"
                            android:layout_width="86dp"
                            android:layout_height="79dp"
                            android:layout_marginEnd="@dimen/buttons1_house_margin_right"
                            android:layout_weight="20"
                            android:adjustViewBounds="true"
                            android:onClick="addAttrButtonsOnClick"
                            app:srcCompat="@drawable/button_townhouse" />

                        <ImageView
                            android:id="@+id/btnPentHouse"
                            android:layout_width="86dp"
                            android:layout_height="79dp"
                            android:layout_marginEnd="@dimen/buttons1_house_margin_right"
                            android:layout_weight="20"
                            android:adjustViewBounds="true"
                            android:onClick="addAttrButtonsOnClick"
                            app:srcCompat="@drawable/button_penthouse" />

                        <ImageView
                            android:id="@+id/btnDoubleFloorHouse"
                            android:layout_width="86dp"
                            android:layout_height="79dp"
                            android:layout_marginEnd="@dimen/buttons1_house_margin_right"
                            android:layout_weight="20"
                            android:adjustViewBounds="true"
                            android:onClick="addAttrButtonsOnClick"
                            app:srcCompat="@drawable/button_doublefloorhouse" />

                        <ImageView
                            android:id="@+id/btnTwoEnterHouse"
                            android:layout_width="86dp"
                            android:layout_height="79dp"
                            android:layout_marginEnd="@dimen/buttons1_house_margin_right"
                            android:layout_weight="20"
                            android:adjustViewBounds="true"
                            android:onClick="addAttrButtonsOnClick"
                            app:srcCompat="@drawable/button_twoenterhouse" />

                        <ImageView
                            android:id="@+id/btnTerrasa"
                            android:layout_width="86dp"
                            android:layout_height="79dp"
                            android:layout_weight="20"
                            android:adjustViewBounds="true"
                            android:onClick="addAttrButtonsOnClick"
                            app:srcCompat="@drawable/button_terrasa" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/barAdditionalAttributes2"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="@dimen/buttons2_house_margin_bottom"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/btnWithFirePlace"
                            android:layout_width="86dp"
                            android:layout_height="79dp"
                            android:layout_marginEnd="@dimen/buttons2_house_margin_right"
                            android:adjustViewBounds="true"
                            android:onClick="addAttrButtonsOnClick"
                            app:srcCompat="@drawable/button_with_fireplace" />

                        <ImageView
                            android:id="@+id/btnBathRoomWithWindow"
                            android:layout_width="86dp"
                            android:layout_height="79dp"
                            android:adjustViewBounds="true"
                            android:onClick="addAttrButtonsOnClick"
                            app:srcCompat="@drawable/button_bathroom_with_window" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/barButtonsFilter"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="horizontal"
                        android:weightSum="100">

                        <ImageView
                            android:id="@+id/btnClear"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="25"
                            android:adjustViewBounds="true"
                            android:onClick="buttonsSearchOnClick"
                            app:srcCompat="@drawable/button_clear" />

                        <FrameLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="50">

                        </FrameLayout>

                        <ImageView
                            android:id="@+id/btnSearch"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="25"
                            android:adjustViewBounds="true"
                            android:onClick="buttonsSearchOnClick"
                            app:srcCompat="@drawable/button_search" />

                    </LinearLayout>
                </LinearLayout>


            </LinearLayout>
        </ScrollView>

        <LinearLayout
            android:id="@+id/resultPanel"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical"
            android:visibility="gone">

            <TextView
                android:id="@+id/title"
                android:layout_width="match_parent"
                android:layout_height="49dp"
                android:background="@color/titleOfForms"
                android:gravity="center"
                android:text="ПЛАНИРОВКА"
                android:textSize="14sp" />

            <LinearLayout
                android:id="@+id/layout"
                android:layout_width="match_parent"
                android:layout_height="33dp"
                android:background="@drawable/item_selector"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/colKorpus"
                    android:layout_width="45dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@color/titleListView"
                    android:gravity="center"
                    android:onClick="titleOfListviewOnClick"
                    android:text="@string/section"
                    android:textColor="@color/colorBackGround"
                    android:textSize="8sp"
                    android:textStyle="bold" />

                <ImageView
